@model PetAdoptionMVC.Models.FeedbackMaster

@{
    ViewData["Title"] = "Feedback Details";
}

<h1>Feedback Details</h1>

<div>
    <h4>Feedback</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Feedback</dt>
        <dd class="col-sm-10">@Model.Feedback</dd>
        <dt class="col-sm-2">Rating</dt>
        <dd class="col-sm-10">@Model.Rating</dd>
        <dt class="col-sm-2">Created Date</dt>
        <dd class="col-sm-10">@Model.CreatedDate.ToString("yyyy-MM-dd")</dd>
        <dt class="col-sm-2">User</dt>
        <dd class="col-sm-10">@Model.UserMaster.ContactNo</dd>
    </dl>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
